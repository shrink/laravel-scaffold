{
    "name": "shrink/laravel-project",
    "type": "project",
    "description": "Add a project description here",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/phpinsights": "^1.14",
        "phpunit/phpunit": "^8.5|^9.0",
        "vimeo/psalm": "^3.11"
    },
    "config": {
        "optimize-autoloader": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "generate-key": [
            "@php artisan key:generate --ansi"
        ],
        "serve": [
            "@generate-key",
            "Composer\\Config::disableProcessTimeout",
            "(source .env && echo \"\nâ†’ Launching local server at http://localhost:${SERVER_PORT}\n\")",
            "@php -S 0.0.0.0:8080 -t public"
        ],
        "quality": [
            "@test",
            "@static",
            "@insights"
        ],
        "test": [
            "vendor/bin/phpunit --colors=always"
        ],
        "insights": [
            "@php artisan insights --ansi --no-interaction --config-path=config/insights.php"
        ],
        "static": [
            "vendor/bin/psalm --long-progress --report=junit.xml"
        ]
    }
}
